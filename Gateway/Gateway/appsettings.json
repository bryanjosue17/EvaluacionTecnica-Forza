{
  "ReverseProxy": {
    "Routes": {
      "auth": {
        "ClusterId": "auth",
        "Match": { "Path": "/auth/{**catch-all}" }
      },
      "catalog": {
        "ClusterId": "catalog",
        "Match": { "Path": "/catalog/{**catch-all}" }
      },
      "orders": {
        "ClusterId": "orders",
        "Match": { "Path": "/orders/{**catch-all}" }
      }
    },
    "Clusters": {
      "auth": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5001/" }
        }
      },
      "catalog": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5002/" }
        }
      },
      "orders": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5003/" }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*"
}
